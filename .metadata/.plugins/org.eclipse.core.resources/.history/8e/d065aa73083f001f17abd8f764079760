package com.example.abbreviationfinder;

public class Main {

	public static void main(String[] args) {

		int[] a = {1,2,3,1,4,5,2,6,3,7,1,6,8};
		int[] b=new int[a.length];
		int k=0;
		boolean c=true;
		
		for(int i=0;i<a.length;i++) {
			int h=a[i];
			for(int j=i+1;j<a.length;j++) {
				if(h==a[j])
					break;
				
			}
			b[k]=a[i];
			k++;
		}
		
		System.out.println(b.toString());
	
		}
		
	}


